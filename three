import math

A, R1, R2 = int(input()), int(input()), int(input())

d = math.sqrt((A - 0) ** 2 + (0 - 0) ** 2)
a = (R1 ** 2 - R2 ** 2 + d ** 2) / (2 * d)
h = math.sqrt(R1 ** 2 - a ** 2)
x3 = 0 + a * (A - 0) / d
y3 = 0 + a * (0 - 0) /d
x4 = x3 + h * (0 - 0) / d
y4 = y3 - h * (A - 0) / d

print(int(x4))
print(int(y4))
